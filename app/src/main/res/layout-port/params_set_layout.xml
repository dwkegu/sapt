<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mat_bg">
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/header_color">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/company_title"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textSize="36sp"
            android:text="标定"/>
    </TableRow>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="40dp"
        android:layout_weight="1"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="110dp"
            android:layout_marginRight="110dp"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="30sp"
                android:text="标定状态"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginTop="30dp"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="id标定状态"
                        android:layout_weight="3"
                        android:layout_marginRight="20dp"
                        android:textSize="28sp"/>
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:id="@+id/idbiaoding_state"
                        android:layout_width="wrap_content"
                        android:layout_height="20dp"
                        android:src="@drawable/state_not"
                        android:layout_weight="1"/>

                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginTop="30dp"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="电压标定状态"
                        android:layout_weight="3"
                        android:layout_marginRight="20dp"
                        android:textSize="28sp"/>
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:id="@+id/voltbiaoding_state"
                        android:layout_width="wrap_content"
                        android:layout_height="20dp"
                        android:src="@drawable/state_not"
                        android:layout_weight="1"/>

                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginTop="30dp"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="温度标定状态"
                        android:layout_weight="3"
                        android:layout_marginRight="20dp"
                        android:textSize="28sp"/>
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:id="@+id/tempbiaoding_state"
                        android:layout_width="wrap_content"
                        android:layout_height="20dp"
                        android:src="@drawable/state_not"
                        android:layout_weight="1"/>

                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginTop="40dp"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="标定策略"
                        android:layout_weight="3"
                        android:textSize="28sp"/>
                    <TextView
                        android:layout_gravity="center_vertical"
                        android:id="@+id/biaoding_way"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="18串"
                        android:textSize="24sp"
                        android:layout_weight="1"/>

                </TableRow>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginRight="110dp"
            android:layout_marginLeft="110dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="30sp"
                android:layout_gravity="center"
                android:text="模块标定"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="@string/oldId"
                    android:textSize="28sp"/>
                <Spinner
                    android:id="@+id/old_id1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/input_text_color"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/newId"
                    android:layout_weight="3"
                    android:textSize="28sp"/>
                <Spinner
                    android:id="@+id/new_id1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/input_text_color"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/params_biaoding1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/id_button_text_color_selector"
                    android:textSize="28sp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/login_shape"
                    android:elevation="2dp"
                    android:text="@string/biaoding"/>
                <Button
                    android:id="@+id/params_save1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/id_button_text_color_selector"
                    android:text="@string/saving"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:textSize="28sp"
                    android:layout_margin="10dp"
                    android:elevation="2dp"
                    android:background="@drawable/login_shape"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/ID"
                    android:layout_weight="3"
                    android:textSize="28sp"/>
                <Spinner
                    android:id="@+id/biaoding_id1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/input_text_color"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/voltage_gates"
                    android:layout_weight="3"
                    android:layout_marginRight="10dp"
                    android:textSize="28sp"/>
                <Spinner
                    android:id="@+id/voltage_gates"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="28sp"
                    android:textColor="@color/input_text_color"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/temprature_gates"
                    android:layout_weight="3"
                    android:layout_marginRight="10dp"
                    android:textSize="22sp"/>
                <!--Spinner
                    android:id="@+id/temprature_gates"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="28sp"
                    android:textColor="@color/input_text_color"/-->
                <CheckBox
                    android:id="@+id/temp_select_1"
                    android:text="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <CheckBox
                    android:id="@+id/temp_select_2"
                    android:text="2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <CheckBox
                    android:id="@+id/temp_select_3"
                    android:text="3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <CheckBox
                    android:id="@+id/temp_select_4"
                    android:text="4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <CheckBox
                    android:id="@+id/temp_select_5"
                    android:text="5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <CheckBox
                    android:id="@+id/temp_select_6"
                    android:text="6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/params_biaoding2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/id_button_text_color_selector"
                    android:textSize="28sp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/login_shape"
                    android:elevation="2dp"
                    android:text="@string/biaoding"/>
                <Button
                    android:id="@+id/params_save2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/id_button_text_color_selector"
                    android:text="@string/saving"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:textSize="28sp"
                    android:layout_margin="10dp"
                    android:elevation="2dp"
                    android:background="@drawable/login_shape"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>